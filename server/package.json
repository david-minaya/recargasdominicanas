{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/server.js",
  "license": "ISC",
  "scripts": {
    "start": "node .",
    "dev": "nodemon -r dotenv/config src/server.ts",
    "update-database": "ts-node --transpile-only ./src/utils/updateDatabase.ts",
    "test": "jest --verbose",
    "clean": "rimraf dist",
    "build": "run-s build:*",
    "build:ts": "tsc",
    "build:email": "node ./scripts/compile-email-template.js ./src/emails ./dist/emails",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:create -n",
    "migration:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "playground": "node --unhandled-rejections=strict -r ts-node/register ./playground.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc --noemit"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.18.2",
    "@hubspot/api-client": "^3.4.1",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "cli-highlight": "^2.1.11",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "fast-xml-parser": "^4.0.12",
    "firebase-admin": "^11.0.1",
    "googleapis": "^71.0.0",
    "handlebars": "^4.7.6",
    "helmet": "^5.1.0",
    "juice": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.4.11",
    "npm-run-all": "^4.1.5",
    "rate-limiter-flexible": "^2.3.7",
    "reflect-metadata": "^0.1.13",
    "sql-formatter": "^4.0.2",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/express-session": "^1.17.4",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^28.1.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.21",
    "@types/nodemailer": "^6.4.0",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.2.1",
    "typescript": "4.7.4"
  },
  "nodemonConfig": {
    "ext": "js,ts,css,json",
    "watch": [
      "src"
    ],
    "ignore": [
      "dist"
    ]
  }
}
